{
  "name": "jupyter-mate",
  "description": "Your Jupyter server mate.",
  "version": "0.1.6",
  "homepage": "./",
  "main": "dist/desktop/desktop.js",
  "author": {
    "name": "Hadrien Mary",
    "email": "hadrien.mary@gmail.com",
    "url": "https://github.com/hadim"
  },
  "scripts": {
    "start": "node fuse",
    "rebuild": "electron-rebuild",
    "dev": "forego start -p 3000",
    "test": "react-scripts test --env=jsdom",
    "react": "cross-env BROWSER=none react-scripts start",
    "electron": "electron .",
    "electron-wait": "node src/electron-wait-react",
    "build-react": "react-scripts build",
    "dist": "yarn postdist && electron-builder",
    "dist:all": "yarn postdist && electron-builder -mlw",
    "postdist": "yarn install && yarn build-react && electron-builder install-app-deps"
  },
  "devDependencies": {
    "@material-ui/core": "1.2.1",
    "@material-ui/icons": "1.1.0",
    "@types/node": "10.3.2",
    "@types/prop-types": "15.5.3",
    "@types/react": "16.3.17",
    "@types/react-dom": "16.0.6",
    "electron": "2.0.2",
    "electron-builder": "20.15.1",
    "electron-rebuild": "1.7.3",
    "fuse-box": "3.2.2",
    "react-scripts": "1.1.4",
    "tslib": "1.9.2",
    "typescript": "2.9.1"
  },
  "dependencies": {
    "electron-store": "2.0.0",
    "npm-path": "^2.0.4",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-lazylog": "3.1.4",
    "typeface-roboto": "0.0.54"
  },
  "build": {
    "appId": "app.hadim.app",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "dist"
    },
    "publish": [
      "github"
    ]
  }
}
