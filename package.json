{
	"name": "react-electron-webpack-fast",
	"version": "0.0.1",
	"author": "d",
	"license": "MIT",
	"main": "dist/desktop/main.js",
	"scripts": {
		"preprod": "cross-env NODE_ENV=production webpack --env.prod -p --config=webpack.node.js",
		"prod": "cross-env NODE_ENV=production webpack --config=webpack.prod.js --env.prod --production --optimize-minimize",
		"prestart": "cross-env NODE_ENV=development webpack --env.dev --development --config=webpack.node.js",
		"start": "cross-env NODE_ENV=development webpack-dev-server --config=webpack.dev.js --hot --env.dev --port=8070",
		"preserve": "cross-env NODE_ENV=development webpack --env.dev --development --config=webpack.node.js",
		"serve": "cross-env NODE_ENV=development webpack-serve --config=webpack.serve.js --port=8080",
		"tscheck": "tsc -p tsconfig.json --outDir _checked",
		"pkg": "cross-env NODE_ENV=production webpack --config=webpack.pkg.js --production --optimize-minimize"
	},
	"dependencies": {
		"@emotion/core": "10.0.5",
