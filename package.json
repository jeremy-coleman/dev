{
  "name": "coglite",
  "productName": "Coglite Desktop",
  "description": "Coglite Desktop",
  "version": "0.0.0",
  "private": true,
  "author": "Coglite team",
  "copyright": "Â© 2017, Coglite inc.",
  "main": "dist/desktop/desktop.js",
  "build": {
    "appId": "com.example.coglite",
    "files": [
      "tasks/html/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "tasks/resources"
    },
    "publish": null
  },
  "scripts": {
    "start": "node tasks/start.js",
    "desktop.dev": "cross-env ELECTRON_DEV_URL=http://localhost:3000 electron .",
    "updatedeps": "electron-builder install-app-deps",
    "preunit": "webpack --config=tasks/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "test": "npm run unit",
    "release": "npm test && webpack --config=tasks/webpack.app.config.js --env=production && electron-builder"
  },
  "dependencies": {
    "codemirror": "5.38.0",
    "dotenv": "6.0.0",
    "electron-devtools-installer": "2.2.4",
    "electron-settings": "3.2.0",
    "event-kit": "2.5.0",
    "execa": "0.10.0",
    "fs-jetpack": "1.3.0",
    "glamorous": "4.13.1",
    "mobx": "5.0.3",
    "mobx-react": "5.2.3",
    "qrcode": "1.2.0",
    "react": "16.4.1",
    "react-codemirror2": "5.0.4",
    "react-dom": "16.4.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "semantic-ui-react": "0.81.1",
    "sql-formatter": "2.3.1",
    "ts-loader": "4.4.1",
    "tslib": "1.9.2"
  },
  "devDependencies": {
    "@types/codemirror": "0.0.57",
    "@types/execa": "0.9.0",
    "@types/jest": "23.1.1",
    "@types/node": "10.3.4",
    "@types/react": "16.4.0",
    "@types/react-dom": "16.0.6",
    "@types/react-router": "4.0.26",
    "@types/react-router-dom": "4.2.7",
    "@types/semantic-ui": "2.2.6",
    "browser-sync": "2.24.4",
    "browser-sync-webpack-plugin": "2.2.2",
    "chai": "4.1.2",
    "chalk": "2.4.1",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "electron": "2.0.2",
    "electron-builder": "20.15.1",
    "electron-mocha": "6.0.4",
    "extract-text-webpack-plugin": "next",
    "file-loader": "1.1.11",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "json-loader": "0.5.7",
    "less": "3.0.4",
    "less-loader": "4.1.0",
    "mocha": "5.2.0",
    "node-sass": "4.9.0",
    "raw-loader": "0.5.1",
    "sass-loader": "7.0.3",
    "semantic-ui-css": "2.3.2",
    "semantic-ui-less": "2.3.2",
    "semantic-ui-react-less-loader": "0.2.0",
    "source-map-support": "0.5.6",
    "style-loader": "0.21.0",
    "typescript": "2.9.2",
    "url-loader": "1.0.1",
    "webpack": "4.12.0",
