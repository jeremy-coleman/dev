{
    "compilerOptions": {
      "target": "es2015",
      "module": "commonjs",
      "moduleResolution": "node",
      "jsx": "react",
      "sourceMap": true,
      "importHelpers": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "allowSyntheticDefaultImports": false,
      "noUnusedLocals": true,
      "strictNullChecks": false,
      "noImplicitReturns": true,
      "outDir": "dist",
      "lib":["dom","dom.iterable", "esnext", "es2017.object"],
      "skipDefaultLibCheck": true,
      "skipLibCheck": true,
      "allowJs": true
    },
    "include": ["src", "typings", "tasks/definitions",],
    "exclude": ["node_modules", "wip", "tools", "dist", "_dev"]
  }
  